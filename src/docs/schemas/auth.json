{
  "AuthRegister": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "example": "anna@mail.com"
      },
      "name": {
        "type": "string",
        "example": "Anna"
      },
      "password": {
        "type": "string",
        "example": "Qwerty@11"
      },
      "password_confirmation": {
        "type": "string",
        "example": "Qwerty@11"
      }
    }
  },
  "AuthLogin": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "example": "user1@mail.com"
      },
      "password": {
        "type": "string",
        "example": "Qwerty@11"
      },
      "remember_me": {
        "type": "boolean",
        "example": false
      }
    }
  },
  "AuthRefreshToken": {
    "type": "object",
    "properties": {
      "refresh_token": {
        "type": "string",
        "example": ""
      }
    }
  }
}